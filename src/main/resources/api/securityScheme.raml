#%RAML 1.0 SecurityScheme
type: OAuth 2.0
description: This API supports OAuth security type
describedBy:
  headers:
    Authentication:
      description: Please use a valid ClientID and ClientSecret
      required: false
  queryParameters:
    ClientID:
      type: string
      required: true
    ClientSecret:
      type: string
      required: true
  responses:
    200:
      description: Ok
    401:
      description: unauthorized
    403:
      description: Forbidden
